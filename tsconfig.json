{
  "include": ["src"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "target": "ES5",
    "module": "ESNext",
    "lib": ["dom", "es6", "es7"],
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-jsx",
    "declaration": false,
    "downlevelIteration": true,
    "strict": false,
    "moduleResolution": "node",
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
